  },
  "license": "MIT",
  "author": "Lari Hoppula",
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "eslint": "^2.3.0",
    "eslint-plugin-react": "^4.2.0",
    "expect": "^1.14.0",
    "faye-websocket": "^0.11.0",
    "firebase-server": "^0.5.3",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "proxyquire": "^1.7.4",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "rimraf": "^2.5.2",
    "redux": "^3.3.1"
